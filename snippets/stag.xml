<?xml version="1.0" encoding="UTF-8"?>
<manifest>

  <remote name="gitlab"
          fetch="https://gitlab.com/" />

  <remote name="github"
           fetch="https://github.com/" />

  <remote  name="stag"
           revision="t13"
           fetch="https://github.com/StagOS/"  />

  <remote  name="stag-beta"
           revision="t13"
           fetch="https://github.com/StagOS-Beta/"  />

  <remote  name="gitlab-stag"
           revision="t13"
